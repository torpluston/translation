#: (content/transports/contents+en-US.lrtopic.body)
msgid "<table>"
msgstr "<table>"

#: http//localhost/en-US/transports/
#: (content/transports/contents+en-US.lrtopic.body)
msgid "<tbody>"
msgstr "<tbody>"

#: http//localhost/en-US/transports/
