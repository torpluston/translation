#: (content/misc/menu/contents+en.lrquestion.description)
msgid "Press"
msgstr "Imprensa"

#: https//support.torproject.org/misc/menu/
#: (content/misc/menu/contents+en.lrquestion.description)
msgid "Blog"
msgstr "Blogue"

#: https//support.torproject.org/misc/menu/
#: (content/misc/menu/contents+en.lrquestion.description)
msgid "Newsletter"
msgstr "Boletim informativo"

#: https//support.torproject.org/misc/menu/
#: (content/misc/menu/contents+en.lrquestion.description)
msgid "Contact"
msgstr "Contactar"

#: https//support.torproject.org/misc/menu/
