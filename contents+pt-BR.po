msgstr "* tor.exe"

#: https//support.torproject.org/tbb/tbb-10/
#: (content/tbb/tbb-10/contents+en.lrquestion.description)
msgid "* obfs4proxy.exe (if you use bridges)"
msgstr "* obfs4proxy.exe (caso você use conexões de ponte)"

#: https//support.torproject.org/tbb/tbb-10/
#: (content/tbb/tbb-10/contents+en.lrquestion.description)
msgid "* For macOS"
msgstr "* Para macOS"

#: https//support.torproject.org/tbb/tbb-10/
#: (content/tbb/tbb-10/contents+en.lrquestion.description)
msgid "* TorBrowser"
msgstr "* Tor Browser"

#: https//support.torproject.org/tbb/tbb-10/
#: (content/tbb/tbb-10/contents+en.lrquestion.description)
