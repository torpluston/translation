#, python-format
msgid "Unable to change volume label: %(message)s"
msgstr "No s'ha pogut canviar l'etiqueta del volum: %(message)s"

#: ../liveusb/creator.py:478 ../liveusb/creator.py:489
#, python-format
msgid "Unable to chmod %(file)s: %(message)s"
