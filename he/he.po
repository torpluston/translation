#, python-format
msgid "'%s' is not a directory"
msgstr "'%s' אינו סיפרייה"

#: ../tails_installer/source.py:75
#, python-format
msgid "Skipping '%(filename)s'"
msgstr "מדלג על '%(filename)s'"

