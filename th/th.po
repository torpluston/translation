#, python-format
msgid "Partitioning device %(device)s"
msgstr "แบ่งที่อุปกรณ์ %(device)s"

#: ../tails_installer/creator.py:995
#, python-format
msgid "Unsupported device '%(device)s', please report a bug."
msgstr "อุปกรณ์ไม่รองรับ '%(device)s' กรุณารายงานบั้ก"

