#, python-format
msgid "Download %(distribution)s"
msgstr "Descargar %(distribution)s"

#: ../liveusb/gui.py:747
msgid "Download complete!"
msgstr "¡Descarga completa!"

#: ../liveusb/gui.py:751
msgid "Download failed: "
msgstr "Fallo de la descarga: "

