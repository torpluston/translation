#, python-format
msgid "Skipping '%(filename)s'"
msgstr "'%(filename)s' atlanıyor"

#: ../tails_installer/utils.py:44
#, python-format
msgid ""
"There was a problem executing `%s`.%s\n"
"%s"
msgstr "`%s` çalıştırılırken bir hata oluştu.%s\n%s"

