msgid "*  فارسى (fa)"
msgstr "*  فارسى (fa)"

#: https//support.torproject.org/tbb/tbb-37/
#: (content/tbb/tbb-37/contents+en.lrquestion.description)
msgid "* Français (fr)"
msgstr "* Französisch (fr)"

#: https//support.torproject.org/tbb/tbb-37/
#: (content/tbb/tbb-37/contents+en.lrquestion.description)
msgid "* Italiano (it)"
msgstr "* Italiano (it)"

#: https//support.torproject.org/tbb/tbb-37/
#: (content/tbb/tbb-37/contents+en.lrquestion.description)
msgid "* 日本語 (ja)"
msgstr "* 日本語 (ja)"

#: https//support.torproject.org/tbb/tbb-37/
#: (content/tbb/tbb-37/contents+en.lrquestion.description)
msgid "* Korean (ko)"
msgstr "* Korean (ko)"

#: https//support.torproject.org/tbb/tbb-37/
