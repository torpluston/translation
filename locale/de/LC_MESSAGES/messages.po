#: tmp/cache_locale/7d/7d56367a61f987367eeb2a89d0c6db83fd0801cce86278bf7e99ed39b5b46254.php:639
msgid "Just mail us at <span class=\"email\">giving(at)torproject.org</span></a>."
msgstr ""
