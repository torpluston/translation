msgstr ""

#: http//localhost/en-US/secure-connections/
#: (content/secure-connections/contents+en-US.lrtopic.body)
msgid "Site.com"
msgstr "Sajt.se"

#: http//localhost/en-US/secure-connections/
#: (content/secure-connections/contents+en-US.lrtopic.body)
#: http//localhost/en-US/security-slider/
#: (content/security-slider/contents+en-US.lrtopic.body)
msgid "</dt>"
msgstr ""

#: http//localhost/en-US/secure-connections/
#: (content/secure-connections/contents+en-US.lrtopic.body)
#: http//localhost/en-US/security-slider/
#: (content/security-slider/contents+en-US.lrtopic.body)
