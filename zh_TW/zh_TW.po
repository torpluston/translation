#, python-format
msgid "'%s' does not exist"
msgstr "「%s」不存在"

#: ../tails_installer/source.py:66
#, python-format
msgid "'%s' is not a directory"
msgstr "「%s」不是目錄"

#: ../tails_installer/source.py:76
#, python-format
msgid "Skipping '%(filename)s'"
msgstr "略過「%(filename)s」"

