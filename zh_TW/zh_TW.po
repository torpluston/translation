#, python-format
msgid "Partitioning device %(device)s"
msgstr "分割設備是 %(device)s"

#: ../liveusb/gui.py:607
msgid "Persistent Storage"
msgstr "永續性儲存體"

#: ../liveusb/dialog.py:170
msgid "Persistent Storage (0 MB)"
msgstr "永久容量 (0 MB)"

#: ../liveusb/gui.py:688 ../liveusb/gui.py:717
