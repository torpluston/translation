msgstr "'%(udi)s' op '%(device)s' afkoppelen"

#: ../liveusb/creator.py:826
#, python-format
msgid "Unmounting mounted filesystems on '%(device)s'"
msgstr "Aangekoppeld bestandssysteem op '%(device)s' afkoppelen"

#: ../liveusb/creator.py:879
#, python-format
