#: bin/openpgp-applet:410
msgid "Name"
msgstr "Jméno"

#: bin/openpgp-applet:411
msgid "Key ID"
msgstr "ID klíče"

#: bin/openpgp-applet:412
msgid "Status"
msgstr "Status"

#: bin/openpgp-applet:444
msgid "Fingerprint:"
msgstr "Otisk:"

