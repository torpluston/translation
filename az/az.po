#: ../liveusb/launcher_ui.py:156
msgid ""
"Upgrade\n"
"by cloning"
msgstr "Yüksəlt\nklonlamaqla"

#: ../liveusb/launcher_ui.py:158
msgid ""
"Upgrade\n"
"from ISO"
msgstr "Yüksəlt\nISO-dan"

#: ../liveusb/dialog.py:159
msgid "Use existing Live system ISO"
